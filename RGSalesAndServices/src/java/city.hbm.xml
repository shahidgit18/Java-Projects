<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="demo.city" table="city">
      <id name="cityid" type="integer" column="city_id"></id>
      <property name="distid" type="integer" column="dist_id"/>
      <property name="citynm" type="string" column="city_nm"/>
  </class>
</hibernate-mapping>
